{
  "Application": {
    "Name": "#{APPLICATION_NAME}#",
    "Version": "1.0.0",
    "Environment": "Production"
  },
  "Database": {
    "Provider": "SQLite",
    "ConnectionString": "Data Source=D:\\home\\site\\wwwroot\\App_Data\\application.db",
    "MigrationsAssembly": "#{APPLICATION_NAME}#.Server"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=D:\\home\\site\\wwwroot\\App_Data\\application.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Features": {
    "EnableSwagger": false,
    "EnableDetailedErrors": false,
    "EnableSeedData": false
  },
  "Security": {
    "AllowedOrigins": [
      "https://*.azurewebsites.net",
      "https://*.azurestaticapps.net"
    ]
  }
}