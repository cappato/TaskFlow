{
  "Application": {
    "Name": "PimFlow",
    "Version": "1.0.0",
    "Environment": "Development"
  },
  "Database": {
    "Provider": "SQLite",
    "ConnectionString": "Data Source=App_Data/application.db",
    "MigrationsAssembly": "PimFlow.Server"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=App_Data/application.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Information"
    }
  },
  "AllowedHosts": "*",
  "Features": {
    "EnableSwagger": true,
    "EnableDetailedErrors": true,
    "EnableSeedData": true
  }
}