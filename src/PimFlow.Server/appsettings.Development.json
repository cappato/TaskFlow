{
  "Application": {
    "Name": "#{APPLICATION_NAME}#",
    "Version": "1.0.0-dev",
    "Environment": "Development"
  },
  "Database": {
    "Provider": "SQLite",
    "ConnectionString": "Data Source=App_Data/application-dev.db",
    "MigrationsAssembly": "#{APPLICATION_NAME}#.Server"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=App_Data/application-dev.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Information",
      "Microsoft.EntityFrameworkCore.Database.Command": "Information"
    }
  },
  "Features": {
    "EnableSwagger": true,
    "EnableDetailedErrors": true,
    "EnableSeedData": true
  }
}